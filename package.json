{
  "name": "react-ethereal-boilerplate",
  "version": "0.0.1",
  "description": "React + Redux + redux-saga lightweight starter kit",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack/dev.config.js --open --inline --hot --env dev",
    "build": "webpack -p --config ./config/webpack/prod.config.js",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint --ext .js src/",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:staged": "lint-staged"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.js": "lint:js",
    "*.scss": "stylelint"
  },
  "keywords": [
    "react",
    "redux",
    "redux-saga",
    "boilerplate",
    "starter-kit"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "lint-staged": "^6.0.1",
    "node-sass": "^4.5.3",
    "pre-commit": "^1.2.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "stylelint": "^8.0.0",
    "stylelint-config-sass-guidelines": "^4.1.0",
    "stylelint-declaration-use-variable": "^1.6.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.0",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  }
}
